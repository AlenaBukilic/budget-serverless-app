{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "budget",
    "type": "object",
    "properties": {
        "createdAt": {
            "type": "string"
        },
        "userId": {
            "type": "string"
        },
        "budgetItemId": {
            "type": "string"
        },
        "amount": {
            "type": "number",
            "minLength": 1,
            "maxLength": 20
        },
        "income": {
            "type": "boolean"
        },
        "attachmentUrl": {
            "type": "string"
        }
    },
    "required": [
        "amount", "income"
    ],
    "additionalProperties": false
}
